{
  "manifest_version": 3,
  "name": "Ad Rotter",
  "version": "1.0",
  "description": "An extension that *brainrots* your ads instead of just blocking them!",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "subwaysurfers/1.gif",
        "subwaysurfers/2.gif",
        "subwaysurfers/3.gif",
        "subwaysurfers/4.gif",
        "subwaysurfers/5.gif"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}
